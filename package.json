  "scripts": {
    "postinstall": "npm rebuild node-sass",
    "prepublish": "npm run build && npm run test:coverage",
    "predeploy": "npm run build-gh",
    "build-gh": "react-scripts-ts build",
    "deploy": "gh-pages -d build",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts-ts start",
    "start": "npm-run-all -p watch-css start-js",
    "test": "react-scripts-ts test --env=jsdom  --setupFiles ./src/setupTests.js ",
    "test:coverage": "npm test -- --coverage",
    "eject": "react-scripts-ts eject",
    "type-check": "tsc --noEmit",
    "tslintfix": "./node_modules/tslint/bin/tslint --project tsconfig.json --fix --",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build-css && npm run build:js && npm run build:js-cleanup && npm run build:rollup",
    "build:js": "rm -rf dist && babel src/lib --copy-files --out-dir dist --extensions \".js,.ts,.tsx\" --source-maps inline",
    "build:js-cleanup": "rm -rf dist/containers/demo_components && rm -rf dist/utils/jsonResponses",
    "build:rollup": "rollup -c"
  }
}
